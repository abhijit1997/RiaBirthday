<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_abhi5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.riabirthday.abhi5">

    <TextView
        android:text="What is the best thing that always happens with us?"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:id="@+id/textView8"
        android:textAllCaps="true"
        android:fontFamily="casual"
        android:textColor="@android:color/holo_red_dark"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="18sp" />

    <Button
        android:text="Eleventh Hour Unplanned meetings"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button18"
        android:layout_alignStart="@+id/button18"
        android:layout_marginTop="28dp"
        android:id="@+id/abhi5" />

    <Button
        android:text="Good Restaurants To Eat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/abhi5"
        android:layout_alignStart="@+id/abhi5"
        android:layout_marginTop="18dp"
        android:id="@+id/button24" />

    <Button
        android:text="Our Plans always work"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="21dp"
        android:id="@+id/button18"
        android:layout_below="@+id/textView8"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="55dp"
        android:id="@+id/button25"
        android:onClick="abhivoice5"
        android:layout_below="@+id/button24"
        android:layout_alignParentStart="true"
        android:layout_marginStart="115dp" />
</RelativeLayout>
